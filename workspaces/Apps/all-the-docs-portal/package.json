{
  "name": "@theorylabs/all-the-docs-portal",
  "private": true,
  "license": "MIT",
  "version": "1.0.0",
  "scripts": {
    "dev:npmjs": "yarn --cwd npmjs dev",
    "dev:home": "yarn --cwd home dev",
    "dev": "npm-run-all --parallel dev:*",
    "build:npmjs": "yarn --cwd npmjs build",
    "build:home": "yarn --cwd home build",
    "build": "npm-run-all --parallel build:*",
    "start:npmjs": "yarn --cwd npmjs start",
    "start:home": "yarn --cwd home start",
    "start": "npm-run-all --parallel start:*",
    "deploy:npmjs": "yarn --cwd npmjs deploy",
    "deploy:home": "yarn --cwd home deploy",
    "deploy": "npm-run-all --parallel deploy:*"
  },
  "devDependencies": {
    "npm-run-all": "^4.1.5"
  }
}
